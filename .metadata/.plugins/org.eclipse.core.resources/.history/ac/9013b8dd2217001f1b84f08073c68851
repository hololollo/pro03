<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
"-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">
<select id="getMemberList" resultType="com.company.dto.Member">
        select id, pw, name, tel, addr, postcode from member
</select>
 <select id="getMember" parameterType="String" resultType="com.company.dto.Member">
        SELECT id, pw, name, birth, gender, postcode, addr FROM member WHERE id = #{id}
    </select>
</mapper>